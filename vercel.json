{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/dist/apps/client/assets/$1"
    },
    {
      "src": "/(.*).(js|css|ico)",
      "dest": "/dist/apps/client/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/apps/client/index.html"
    }
  ]
}
